{
    "version": 2,
    "builds": [
      {
        "src": "src/App.jsx",
        "use": "@vercel/static-build"
      },
      {
        "src": "api/chat.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/chat",
        "dest": "/api/chat.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  